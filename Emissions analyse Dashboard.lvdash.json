{
  "datasets": [
    {
      "name": "051a3ded",
      "displayName": "Location Emissions",
      "queryLines": [
        "select latitude,longitude,`GHG emissions mtons CO2e` AS emissions\r\n",
        "from emissions_data"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "3cf49144",
      "displayName": "Emissions per person",
      "queryLines": [
        "Select county_state_name,population, CAST(Replace(`GHG emissions mtons CO2e`,',','') AS DOUBLE )/ nullif (CAST(REPLACE(population,',','')AS DOUBLE),0) AS Emissions_per_person\r\n",
        "from emissions_data\r\n",
        "ORDER BY emissions_per_person DESC"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "005a1818",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "Select state_abbr, SUM(CAST(Replace(`GHG emissions mtons CO2e`,',','') AS DOUBLE )) AS Total_Emissions\r\n",
        "from emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10\r\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "2551119b",
      "displayName": "County Shaming",
      "queryLines": [
        "Select county_state_name,population, CAST(Replace(`GHG emissions mtons CO2e`,',','') AS DOUBLE ) AS Total_Emissions\r\n",
        "from emissions_data\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
